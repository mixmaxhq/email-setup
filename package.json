{
  "name": "email-setup",
  "version": "1.0.2",
  "description": "Collection of utilities for checking email configuration settings.",
  "main": "index.js",
  "repository": "https://github.com/mixmaxhq/email-setup",
  "author": "Trey Tacon <trey@mixmax.com> (https://mixmax.com)",
  "license": "MIT",
  "dependencies": {
    "dmarc-parse": "^1.0.2",
    "promise-callbacks": "^3.0.0",

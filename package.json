  },
  "files": [
    "dist",
    "index.js",
    "src"
  ],
  "devDependencies": {
    "ava": "^0.22.0",
    "ava-spec": "^1.1.0",
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-plugin-transform-async-to-generator": "^6.24.1",
    "eslint": ">=3",
    "eslint-config-mixmax": "^0.6.0",
    "nyc": "^11.1.0",
    "rewire": "^2.5.2",
    "sinon": "^3.2.1"
  },
  "engines": {
    "node": ">= 6.9.1"
  },
  "scripts": {
    "babelBuild": "babel src -d dist/node",
    "lint": "eslint .",
    "prepublish": "npm run babelBuild",
    "report": "nyc ava",
    "test": "ava"
